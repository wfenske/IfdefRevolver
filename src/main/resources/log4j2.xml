<?xml version="1.0" encoding="UTF-8" ?>

<Configuration package="de.ovgu.skunk">
    <Appenders>
        <Console name="Console" target="SYSTEM_ERR">
            <PatternLayout pattern="%d{HH:mm:ss} %5p %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="Console"/>
        </Root>
        <!--
        <Logger name="de.ovgu.skunk.bugs.eval" level="trace">
            <AppenderRef ref="Console" />
        </Logger>
         -->
    </Loggers>
</Configuration>
