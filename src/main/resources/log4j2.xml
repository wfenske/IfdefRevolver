<?xml version="1.0" encoding="UTF-8" ?>

<Configuration package="de.ovgu.skunk.bugs.eval">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss} %5p %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="Console" />
		</Root>
		<!--
		<Logger name="de.ovgu.skunk.bugs.eval" level="trace">
			<AppenderRef ref="Console" />
		</Logger>
		 -->
	</Loggers>
</Configuration>
